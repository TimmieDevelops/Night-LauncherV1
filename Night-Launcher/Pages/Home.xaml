<UserControl x:Class="Night_Launcher.Pages.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Night_Launcher.Pages"
             mc:Ignorable="d"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Frame x:Name="TabFrame" HorizontalAlignment="Left" Width="90"/>
        <Frame x:Name="PageFrame" Margin="90,45,0,0"/>
        <ui:CardControl VerticalAlignment="Top" IsEnabled="False" Height="45" Margin="90,0,-10,0"/>
    </Grid>
</UserControl>